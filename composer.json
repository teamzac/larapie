{
    "name": "teamzac/larapie",
    "description": "A Laravel-enhanced wrapper for the SimplePie library",
    "keywords": [
        "teamzac",
        "larapie"
    ],
    "homepage": "https://github.com/teamzac/larapie",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chad Janicek",
            "email": "chad@zactax.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Teamzac\\Larapie\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teamzac\\Larapie\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Teamzac\\Larapie\\LarapieServiceProvider"
            ],
            "aliases": {
                "Larapie": "Teamzac\\Larapie\\LarapieFacade"
            }
        }
    }
}
